/**app.wxss**/
page{
  background-color: #f2f2f2;
}
.bodyBox{
  width: 86.67%;
  margin: 0 auto;
}
.boxes{
  width: 93.33%;
  margin: 0 auto;
}
.container {
  height: 100%;
  width: 100%;
} 
.clearfloat::after {
    display: block;
    clear: both;
    content: "";
    visibility: hidden;
    height: 0;
}
text.navActive::after{
  position: absolute;
  left: 50%;
  content: "";
  width: 24rpx;
  margin-left: -12rpx;
  bottom: -10rpx;
  height: 4rpx;
  background-color: #ff8933;
}
.block{
  display: block!important;
}
.noTouch{
  position: fixed;
  left: 0;
  bottom: 0;
  width: 25%;
  height: 110rpx;
  z-index: 10000;
}
.noTouch2{
  left: 25%;
}
.noTouch3{
  left: 50%;
}
.noTouch4{
  left: 75%;
}
.tipNews{
  position: absolute;
  right: 28rpx;
  top: 28rpx;
  width: 26rpx;
  font-size: 16rpx;
  z-index: 100;
  height: 26rpx;
  background-color: rgba(255, 0, 0, 1);
  border-radius: 50%;
  color: white;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  display: -webkit-flex;
  -webkit-align-items: center;
  -webkit-justify-content: center;
  -webkit-flex-direction: column;
}
.nav{
  z-index: 1000;
}
.bottomNav{
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 10000;
}
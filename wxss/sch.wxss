page{
  background-color: white;
  padding-top: 124rpx;
  padding-bottom: 120rpx;
  box-sizing: border-box;
  -webkit-box-sizing: border-box;
}
.searchBox{
  padding: 20rpx 25rpx;
  box-sizing:border-box;
  -webkit-box-sizing: border-box;
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  z-index: 5;
  background-color: white
}
.searchInput{
  border: 1px solid #999999;
  border-radius: 18rpx;
  display: flex;
  display: -webkit-flex;
}
.searchInput input{
  height: 80rpx;
  width: 81.6%;
  padding: 0 80rpx;
  box-sizing: border-box;
  -webkit-box-sizing: border-box;
  font-size: 24rpx;
  color: #333333;
}
.searchBtn{
  width: 14.3%;
  position: relative;
  height: 80rpx;
  background: url("data:image/png;base64,") no-repeat center center;
  background-size:30rpx;  
}
.searchBtn::after{
  content: "";
  position: absolute;
  left: -1px;
  top: 50%;
  height: 36rpx;
  margin-top: -18rpx;
  border-left: 1px solid #999999;
}
inputPlace{
  color: #dddddd;
}
.exchange{
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 100rpx;
}
.exchange button{
  display: block;
  width: 100%;
  height: 100%;
  background-color: #ff8933;
  border-radius: 0;
  border-color: #ff8933;
  color: white;
  font-size: 28rpx;
  line-height: 100rpx;
}
.noShopList{
  width: 100%;
  font-size: 30rpx;
  text-align: center;
  padding: 150rpx 0;
}
.listBoxOut{
  width: 100%;
  padding: 20rpx 0;
  border-bottom: 1px solid #eeeeee;
  
}
.listBox{
  width: 100%;
  padding: 25rpx;
  background-color: #eeeeee;
  box-sizing: border-box;
  -webkit-box-sizing: border-box;
  position: relative;
  transition: transform 0.6s;
  -webkit-transition:transform 0.6s;
}
.shopDelete{
  position: absolute;
  width: 275rpx;
  height: 100%;
  top: 0;
  left: 100%;
  display: flex;
  display: -webkit-flex;
}
.shopDelete .shopZw{
  width: 50rpx;
  height: 100%;
  background-color: #eeeeee;
}
.shopDelete .sDelete{ 
  width: 225rpx;
  background: url('data:image/png;base64,')no-repeat center center;
  background-size: 50rpx;
  background-color: #e28828;
}
.listTransition>view.listBox{
  transform: translate(-275rpx,0);
  -webkit-transform: translate(-275rpx,0);
}
.listImg{
  width: 250rpx;
  font-size: 0;
}
.listImg image{
  width: 100%;
}
.listText{
  width: 100%;
  height: 100%;
  position: absolute;
  left: 0;
  top: 0;
  padding-left: 300rpx;
  box-sizing: border-box;
  -webkit-box-sizing: border-box;
}
.ltIn{
  width: 100%;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  display: -webkit-flex;
  -webkit-align-items: center;
  -webkit-justify-content: center;
  -webkit-flex-direction: column;
  position: relative;
}
.phone{
  width: 60rpx;
  position: absolute;
  right: 25rpx;
  top: 35rpx;
}
.phone image{
  width: 100%;
}
.shop{
  width: 100%;
}
.shopText{
  display: flex;
  display: -webkit-flex;
  margin-top: 18rpx;
}
.shopType{
  font-size: 20rpx;
  padding:0 20rpx;
  background-color: #333333;
  border-radius: 8rpx;
  line-height: 30rpx;
  height: 30rpx;
  display: block;
  color: white;
}
.shopName{
  display: block;
  line-height: 30rpx;
  height: 30rpx;
  font-size: 30rpx;
  margin-left: 10rpx;
  font-weight: bold;
}
.shopNumber{
  margin-top: 24rpx;
  font-size: 20rpx;
  line-height: 20rpx;
  display: flex;
  display: -webkit-flex;
}
.fire{
  display: flex;
  display: -webkit-flex;
  margin-left: 10rpx;
}
.fire text{
  width: 14rpx;
  display: none;
  height: 20rpx;
  background: url('data:image/png;base64,') no-repeat left top;
  background-size: 100% 100%;
  margin-right: 2rpx;
}
.shopAddress{
  padding: 0 25rpx;
  font-size: 20rpx;
  margin-top: 26rpx;
  box-sizing: border-box;
  -webkit-box-sizing: border-box;
  background: url('data:image/png;base64,') no-repeat left center;
  background-size: 16rpx;
}
.shopMoney{
  font-size: 20rpx;
  color: #d81e06;
  line-height: 36rpx;
  margin-top: 24rpx;
  display: flex;
  display: -webkit-flex;
}
.shopMoney>view{
  padding: 0 20rpx;
  border-radius: 8rpx;
  background-color: rgba(216,30,6,0.2);
  display: flex;
  display: -webkit-flex;
}
.shopMoney>view text{
  position: relative;
  display: inline-block;
  color: #888888;
  padding: 0 16rpx 0 8rpx;
}
.shopMoney>view text::after{
  content: "";
  position: absolute;
  right: 4rpx;
  height: 20rpx;
  top: 50%;
  margin-top: -10rpx;
  border-right: 1px solid #d81e06;
}
.shopMoney>view text::before{
  content: "";
  position: absolute;
  top: 50%;
  width: 75%;
  left: 10%;
  border-bottom: 1px solid #888888;
} 
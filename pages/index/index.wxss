/**index.wxss**/
@import '../../wxss/list.wxss';
.userinfo {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.userinfo-avatar {
  width: 128rpx;
  height: 128rpx;
  margin: 20rpx;
  border-radius: 50%;
}

.userinfo-nickname {
  color: #aaa;
}

.usermotto {
  margin-top: 200px;
}
page{
  background-color: white;
}
.searchBox{
  padding: 20rpx 25rpx;
  box-sizing:border-box;
  -webkit-box-sizing: border-box;
}
.swiper{
  width: 100%;
}
.swiper image{
  width: 100%;
  font-size: 0;
}
.typeList{
  width: 100%;
  padding: 30rpx 0 20rpx;
  box-sizing: border-box;
  -webkit-box-sizing: border-box;
  display: flex;
  justify-content: center;
  align-items: center;
  display: -webkit-flex;
  -webkit-align-items: center;
  -webkit-justify-content: center;
  flex-wrap: wrap;
  -webkit-flex-wrap:wrap;
}
.typeBtn{
  width: 218rpx;
  height: 98rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 28rpx;
  flex-direction: column;
  display: -webkit-flex;
  -webkit-align-items: center;
  -webkit-justify-content: center;
  -webkit-flex-direction: column;
  margin-bottom: 10rpx;
  margin-right: 15rpx;
  background: url('data:image/png;base64,') no-repeat left top;
  background-size: 100% 100%;
}
.typeBtn:nth-child(3n){
  margin-right: 0;
}



.shopLists{
  width: 100%;
}
.shopTitles{
  font-size: 34rpx;
  text-align: center;
  letter-spacing: 6rpx;
  margin-top: 40rpx;
  margin-bottom: 20rpx;
}
.shopTitles>text{
  position: relative;  
}
.shopTitles>text::after{
  content: "";
  position: absolute;
  width: 40rpx;
  left: 50%;
  bottom: -10rpx;
  margin-left: -20rpx;
  border-bottom: 1px solid #ff8933;
}
.snBox{
  width: 100%;
}
.lookBtn button{
  background-color: #ff8933;
  border-radius: 10rpx;
  border-color: #ff8933;
  line-height: 90rpx;
  font-size: 30rpx;
  color:#ffffff;
  margin: 50rpx 0;
}
.searchBox{
  position: relative;
}
.searchLeft{
  position: absolute;
  left: 25rpx;
  width: 184rpx;
  border: 1px solid #999999;
  line-height: 70rpx;
  height: 70rpx;
  box-sizing: border-box;
  -webkit-box-sizing: border-box;
  border-radius: 35rpx;
}
.searchLeft .dw{
  width: 100%;
  font-size: 22rpx;
  line-height: 70rpx;
  box-sizing: border-box;
  -webkit-box-sizing: border-box;
  padding:0 40rpx 0 46rpx;
  background: url('data:image/png;base64,') no-repeat 18rpx center;
  background-size: 20rpx;
}
.searchLeft .dw text{
  display: block;
  width: 100%;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
  position: relative;
  text-align: ccenter;
}
.searchLeft .dw::after{
  content: "";
  position: absolute;
  right: 20rpx;
  width: 12rpx;
  
  height: 20rpx;
  top: 50%;
  margin-top:-10rpx;
  background: url('data:image/png;base64,') no-repeat left top;
  background-size: 100% 100%; 
}
.searchRight{
  padding-left: 220rpx;
  box-sizing: border-box;
  -webkit-box-sizing: border-box;
}
.searchRight navigator{
  font-size: 22rpx;
  color: #dddddd;
  border: 1px solid #999999;
  line-height: 70rpx;
  border-radius: 35rpx;
  position: relative;
}
.searchRight navigator text{
  padding-left: 45rpx;
}
.searchImg{
  position: absolute;
  top: 6rpx;
  width: 30rpx;
  right: 45rpx;
}
.searchImg::after{
  content: "";
  position: absolute;
  top: 12rpx;
  left: -24rpx;
  border-right: 1px solid #999999;
  height: 36rpx;
}
.searchImg image{
  width: 100%;
}
.noShopMore{
  font-size: 30rpx;
  color: #999999;
  text-align: center;
  line-height: 100rpx;
}
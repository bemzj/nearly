/* pages/atyApply/atyApply.wxss */

.main{
  padding-bottom: 120rpx;
  margin-top: 20rpx;
}
.mainOut{
  width: 100%;
  background-color: white;
}
.title
{
  padding:35rpx 50rpx;
  line-height: 48rpx;
  font-size: 28rpx;
  color: #333333;
  border-bottom: 1px solid #f2f2f2;
}
.titleClass{
  color: #999999;
  font-size: 28rpx;
  line-height: 48rpx;
}
.textarea{
  width: 100%;
  box-sizing: border-box;
  -webkit-box-sizing: border-box;
  padding: 35rpx 50rpx;
  line-height: 48rpx;
  font-size: 28rpx;

}
.textarea textarea{
  width: 100%;
  height: 288rpx;
  text-align: justify;
}
.mainBottom{
  position: fixed;
  left: 0rpx;
  bottom: 0rpx;
  width: 100%;
  display: flex;
  display: -webkit-flex;
}
.mainBottom navigator{
  width: 50%;
  line-height: 120rpx;
  font-size: 28rpx;
  text-align: center;
  background-color: #ffffff;
  color: #999999;
}
.mainBottom button{
  display: block;
  width: 50%;
  line-height: 120rpx;
  background-color: #ff8933;
  border-radius: 0;
  border-color: #ff8933;
  color: white;
  font-size: 28rpx; 
}
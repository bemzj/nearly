@import '../../wxss/sch.wxss';
@import '../../wxss/list.wxss';
page{
  padding-top: 205rpx;
}
.searchBoxOut{
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  z-index: 1000;
}
.searchBoxHeight{
  height: 100%;
}
.searchBox{
  position: absolute;
}
.type{
  position: absolute;
  left: 0;
  top: 120rpx;
  display: flex;
  width: 100%;
  display: -webkit-flex;
  box-sizing: border-box;
  -webkit-box-sizing: border-box;
  padding: 0rpx 25rpx;
  border-top: 1px solid #eeeeee;
  border-bottom: 1px solid #eeeeee;
  z-index: 10000;
  background-color: white;
  box-shadow: 0 2rpx 4rpx rgba(51, 51, 51, 0.2);
}
.typeNoneShadow{
  box-shadow: none;
}
.type view{
  width: 50%;
  text-align: center;
  font-size: 26rpx;
  line-height: 80rpx;
  color: #888888;
}
.type view text{
  display: inline-block;
  text-indent: -36rpx;
  position: relative;
}
.type view text::after{
  content: "";
  width: 18rpx;
  height: 80rpx;
  background-color: red;
  right: -36rpx;
  position: absolute;
  top: 0rpx;
  background: url(data:image/png;base64,) no-repeat center center;
  background-size: 18rpx;
  transition: transform 0.5s;
  -webkit-transition: transform 0.5s;
}
.type .allChoice{
  color: #ff8933;
}
.type .allChoice text::after{
  transform:rotate(180deg);
  -webkit-transform: rotate(180deg);
  background-image: url('data:image/png;base64,')
}
.allList{
  width: 100%;
  height: 100%;
  padding-top: 205rpx;
  position: relative;
  box-sizing: border-box;
  -webkit-box-sizing: border-box;
  background-color: rgba(51, 51, 51, 0.7);
}
.leftList{
  width: 100%;
  background-color: white;
  position: absolute;
  left: 0;
  top: 205rpx;
  height: 660rpx;
  overflow: hidden;
  display: flex;
  display: -webkit-flex;
  transition: transform 0.5s;
  -webkit-transition: transform 0.5s;
  transform: translate(0,-660rpx);
  -webkit-transform:  translate(0,-660rpx);
}

.rightList{
  background-color: white;
  overflow-y: scroll;
  -webkit-overflow-scrolling: touch;
  position: absolute;
  left: 0;
  width: 100%;
  top: 205rpx;
  max-height: 660rpx;
  transition: transform 0.5s;
  -webkit-transition: transform 0.5s;
  transform: translate(0,-660rpx);
  -webkit-transform:  translate(0,-660rpx);
}
.leftDown{
  transform: translate(0,0);
  -webkit-transform:  translate(0,0);
}
.rightList view{
  padding-left: 50rpx;
  box-sizing: border-box;
  -webkit-box-sizing: border-box;
  font-size: 26rpx;
  
  line-height: 80rpx;
}
.rightList view text{
  display: block;
  width: 100%;
  border-bottom: 1px solid #eeeeee;
  color: #333333;
}
.rightList view:nth-last-child(1) text{
  border: 0
}
.rightList .rightActive text{
  color: #ff8933;
  border-color: #ff8933;
}
.firstList,.secondList{
  width: 50%;
  height: 100%;
  overflow-y: scroll;
  -webkit-overflow-scrolling: touch;
  background-color: #eeeeee;
  padding-bottom: 20rpx;
}
.secondList{
  background-color: white;
}
.firstList view,.secondList view{
  padding: 0 50rpx;
  box-sizing: border-box;
  -webkit-box-sizing: border-box;
  font-size: 26rpx;
  color: #333333;
  line-height: 80rpx;
  width: 100%;
}
.secondList view{
  padding:0 0 0 50rpx;
}
.fristActive{
  background-color: white;
}
.secondList view text{
  display: block;
  width: 100%;
  border-bottom: 1px solid #eeeeee;
}
.secondList view:nth-last-child(1) text{
  border: 0;
}
.secondList .secondActive text{
  border-color: #ff8933;
}
.secondList .secondActive{
  color: #ff8933;
  position: relative;
}
.secondList .secondActive::after{
  content: "";
  position: absolute;
  top: 0;
  right: 50rpx;
  width: 32rpx;
  height: 100%;
  background: url('data:image/png;base64,') no-repeat center center;
  background-size: 32rpx;
}
.exchange{
  position: relative;
  width: 93.33%;
  margin: 0 auto;
}
.exchange button{
  border-radius: 10rpx;
}
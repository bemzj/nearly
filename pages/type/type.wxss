@import '../../wxss/sch.wxss';
@import '../../wxss/list.wxss';
page{
  padding-top: 205rpx;
}
.searchBoxOut{
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  z-index: 1000;
}
.searchBox{
  position: absolute;
}
.type{
  position: absolute;
  left: 0;
  top: 120rpx;
  display: flex;
  width: 100%;
  display: -webkit-flex;
  box-sizing: border-box;
  -webkit-box-sizing: border-box;
  padding: 0rpx 25rpx;
  border-top: 1px solid #eeeeee;
  border-bottom: 1px solid #eeeeee;
  z-index: 10000;
  background-color: white;
  box-shadow: 0 2rpx 4rpx rgba(51, 51, 51, 0.2);
}
.type view{
  width: 50%;
  text-align: center;
  font-size: 26rpx;
  line-height: 80rpx;
  color: #888888;
}
.type view text{
  text-indent: -36rpx;
}
.allList{
  background-color: rgba(51, 51, 51, 0.7);
  width: 100%;
  height: 100%;
  padding-top: 205rpx;
  box-sizing: border-box;
  -webkit-box-sizing: border-box;
}
.leftList{
  width: 100%;
  
  background-color: white;
  height: 640rpx;
  display: flex;
  display: -webkit-flex;
  display: none;
}
.rightList{
  background-color: white;
  
}
.rightList view{
  padding-left: 50rpx;
  box-sizing: border-box;
  -webkit-box-sizing: border-box;
  font-size: 26rpx;
  
  line-height: 80rpx;
}
.rightList view text{
  display: block;
  width: 100%;
  border-bottom: 1px solid #eeeeee;
  color: #333333;
}
.rightList .rightActive text{
  color: #ff8933;
  border-color: #ff8933;
}
.firstList,.secondList{
  width: 50%;
  height: 100%;
  overflow-y: scroll;
  -webkit-overflow-scrolling: touch;
  background-color: #eeeeee;
  padding-bottom: 20rpx;
}
.secondList{
  background-color: white;
}
.firstList view,.secondList view{
  padding: 0 50rpx;
  box-sizing: border-box;
  -webkit-box-sizing: border-box;
  font-size: 26rpx;
  color: #333333;
  line-height: 80rpx;
  width: 100%;
}
.secondList view{
  padding:0 0 0 50rpx;
}
.fristActive{
  background-color: white;
}
.secondList view text{
  display: block;
  width: 100%;
  border-bottom: 1px solid #eeeeee;
}
.secondList .secondActive text{
  border-color: #ff8933;
}
.secondList .secondActive{
  color: #ff8933;
}
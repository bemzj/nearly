/* pages/address/address.wxss */
.main{
  width: 100%;
}
page{
  overflow: hidden;
  width: 100%;
  height: 100%;
  padding-top: 20rpx;
  padding-bottom: 90rpx;
  box-sizing: border-box;
  -webkit-box-sizing: border-box;
}
.noAddress{
  width: 100%;
}
.noAddress text{
  display: block;
  width: 100%;
  font-size: 30rpx;
  color:#888888;
  padding: 220rpx 0;
  text-align: center;
}
.addAddress{
  width: 93.33%;
  display: block;
  margin: 0 auto;
  font-size: 28rpx;
  line-height: 100rpx;
  color: white;
  border-color: #ff8933;
  background-color: #ff8933;
}
.tips{
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  color: #ffffff;
  font-size: 22rpx;
  line-height: 90rpx;
  text-align: center;
  background-color: #dddddd;
}
.tipsfull{
  background-color: #ff8933;
}
.hasAddress{
  margin: 0rpx 0 40rpx;
  background-color: white;
}
.addressBox{
  padding: 38rpx 10rpx;
  box-sizing: border-box;
  -webkit-box-sizing: border-box;
  position: relative;
  border-bottom: 1px solid #f2f2f2;
}
.addressBox:nth-last-child(1){
  border-bottom: 0px;
}
.addressText{
  font-size: 28rpx;
  color: #888888;
  padding-left: 112rpx;
  line-height: 40rpx;
  box-sizing: border-box;
  -webkit-box-sizing: border-box;
}
.addressBox text{
  position: absolute;
  left: 15rpx;
  top: 38rpx;
  line-height: 40rpx;
  font-size: 28rpx;
}
.addressRight{
  margin-top: 20rpx;
}
.addressRight .delect,.addressRight .edit{
  padding: 16rpx 16rpx 0 16rpx;
  width: 61rpx;
  height: 44rpx;
  box-sizing: border-box;
  -webkit-box-sizing: border-box;
  float: right;
  background: url('data:image/png;base64,') no-repeat center bottom;
  background-size: 29rpx;
}
.addressRight .edit{
  background-image: url('data:image/png;base64,')
}
.popWin{
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  display: -webkit-flex;
  -webkit-align-items: center;
  -webkit-justify-content: center;
  -webkit-flex-direction: column;
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  z-index: 100;
  background-color: rgba(0, 0, 0, 0.68);
}
.popWinIn{
  width: 80%;
  box-sizing: border-box;
  -webkit-box-sizing: border-box;
  padding: 50rpx 40rpx;
  background-color: white;
  border-radius: 5rpx;
}
.popInput{
    border: 1px solid #888888;
    border-radius: 4rpx;
    box-sizing: border-box;
    -webkit-box-sizing: border-box;
    padding: 0 30rpx;
    margin-bottom: 30rpx;
}
.popInput input{
  width: 100%;
  height: 78rpx;
  color: #333333;
  font-size: 28rpx;
}
.popBtn{
  display: flex;
  display: -webkit-flex;
  
}
.popBtn button{
  width: 46%;
  height: 70rpx;
  box-sizing: border-box;
  -webkit-box-sizing: border-box;
  line-height: 70rpx;
  font-size: 28rpx;
  color: #999999;
  border: 1px solid #ff8933;
  background-color: transparent;
}
.popBtn button:nth-child(2){
  margin-left: 8%;
  background-color: #ff8933;
  color: white;
}
.picker{
  height: 78rpx;
  font-size: 28rpx;
  line-height: 78rpx;
}